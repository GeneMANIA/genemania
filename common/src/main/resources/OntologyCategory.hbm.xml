<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.genemania.domain">
    <class name="OntologyCategory" table="ONTOLOGY_CATEGORIES">
        <id name="id" type="long" column="ID">
            <generator class="native"/>
        </id>
        <property name="name" type="string" column="NAME" length="64" not-null="true"/>
        <property name="description" type="string" column="DESCRIPTION" length="256" not-null="true"/>
    </class>
</hibernate-mapping>