<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.genemania.domain">
    <class name="Statistics" table="STATISTICS">
        <id name="id" type="long" column="ID">
            <generator class="native"/>
        </id>
        <property name="organisms" type="long"/>
        <property name="networks" type="long"/>
        <property name="interactions" type="long"/>
        <property name="genes" type="long"/>
        <property name="predictions" type="long"/>
        <property name="date" type="date"/>
    </class>
</hibernate-mapping>