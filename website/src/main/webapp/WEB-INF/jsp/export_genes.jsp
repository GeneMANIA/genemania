<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %><%@ taglib uri="http://java.sun.com/jsp/jstl/fmt" prefix="fmt" %><%@ taglib uri="http://www.springframework.org/tags" prefix="spring"%><%@ taglib prefix="fn" uri="http://java.sun.com/jsp/jstl/functions" %><%@ taglib uri="http://java.sun.com/jsp/jstl/fmt" prefix="fmt" %><%@ page contentType="text/plain" %><% response.setHeader("Content-Disposition", "attachment"); response.setHeader("charset", "UTF-8"); %>Symbol	Description	Score	Synonyms	GO annotations	Links
<c:forEach items="${results.resultGenes}" var="rGene" varStatus="iterationgene">${rGene.gene.symbol} <c:if test="${!empty rGene.typedName && rGene.typedName != rGene.gene.symbol}">(${rGene.typedName})</c:if>	${rGene.gene.node.geneData.description}	<fmt:formatNumber value="${rGene.score * 100}" minFractionDigits="2" maxFractionDigits="2" />	<c:forEach items="${rGene.gene.node.genes}" var="gene">${gene.symbol};</c:forEach>	<c:forEach items="${rGene.resultOntologyCategories}" var="rOCat" varStatus="iteration">${rOCat.ontologyCategory.description}<c:if test="${ !iteration.last }">; </c:if></c:forEach>	<c:forEach items="${rGene.links}" var="link" varStatus="iteration">${link.url}<c:if test="${ !iteration.last }">; </c:if></c:forEach><c:if test="${ !iterationgene.last }">
</c:if></c:forEach>
<c:forEach items="${invalidGeneNames}" var="invalidGeneName"  varStatus="iteration">${invalidGeneName}	<spring:message code="genes_tab.unrecognised"/><c:if test="${!iteration.last}">
</c:if></c:forEach>
